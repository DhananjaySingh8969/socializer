<link rel="stylesheet" href="/css/home.css">
    <main class="post-main">
        <header><h1>SOCIALIZER/<%=title%></h1></header>
        <%-include('./post/_postForm');%>
        <div class="list-group post-container inline-block">
            <%for(let post of posts){%>
                <%-include('./post/_main.ejs',{post:post}); %>
            <%}%>
        </div>
       <div class="friends-contianer">
            <h2 class="black mgb20"><i class="fas fa-user-friends"></i>FRIENDS</h2>
            <ol>
                <%for(let friend of friends){%>
                    <%if(locals.user && friend.id==locals.user.id)continue;%>

                    <a class="friend" href="/users/profile/<%=friend.id%>">
                        <li class="blue custom-text li-style-numeric"><i class="far fa-id-badge"></i><%=friend.name%></li>
                    </a>
                <%}%>
            </ol>
        </div>
    </main>
    <script src="/script/home_post.js"></script>
    <script src="/script/home_comment.js"></script>
    <script src="/script/home_like.js"></script>
